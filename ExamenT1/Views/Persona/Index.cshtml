<h1>Lista de personas</h1>
<a href="/persona/create">Crear persona</a>
<table class="table">
    <thead>
        <tr>

            <th>Nombre</th>
            <th>Apellido</th>
            <th>Fecha_Nacimiento</th>
            <th>DNI</th>
            <th>Genero</th>
            <th>Ciudad</th>
            <th>Direccion</th>
            <th>Correo</th>
            <th>Username</th>
            <th>Contraseña</th>
            <th>opciones</th>
        </tr>
    </thead>
    <tbody>
        @{
            var personas = ((List<Personas>)ViewBag.personas);
        }

        @for (var i = 0; i < personas.Count; i++)
        {
            var item = personas[i];
        <tr>
            <td>@item.Nombre</td>
            <td>@item.Apellido</td>
            <td>@item.Fecha_nacimiento</td>
            <td>@item.DNI</td>
            <td>@item.Genero</td>
            <td>@item.Ciudad</td>
            <td>@item.direccion</td>
            <td>@item.Correo</td>
            <td>@item.Username</td>
            <td>@item.Contraseña</td>
            <td><a href="/persona/Edit?id=@item.Id_persona">Editar</a></td>
            <td><a href="/persona/delate?id=@item.Id_persona">Eliminar</a></td>
        </tr>
        }
    </tbody>
</table>